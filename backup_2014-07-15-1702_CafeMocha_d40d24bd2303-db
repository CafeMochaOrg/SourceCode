# Table: `wp_views`
# Approximate rows expected in table: 1785

# Delete any existing table `wp_views`

DROP TABLE IF EXISTS `wp_views`;

# Table structure of table `wp_views`

CREATE TABLE `wp_views` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `user_id` varchar(255) NOT NULL,
  `post_id` varchar(255) NOT NULL,
  `views` varchar(255) NOT NULL,
  `date` date NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=1709 DEFAULT CHARSET=latin1 ;

