# Table: `wp_easymail_subscribers`
# Approximate rows expected in table: 2

# Delete any existing table `wp_easymail_subscribers`

DROP TABLE IF EXISTS `wp_easymail_subscribers`;

# Table structure of table `wp_easymail_subscribers`

CREATE TABLE `wp_easymail_subscribers` (
  `ID` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `email` varchar(100) NOT NULL,
  `name` varchar(100) NOT NULL,
  `join_date` datetime NOT NULL,
  `active` int(1) NOT NULL DEFAULT '0',
  `unikey` varchar(24) NOT NULL,
  `lists` varchar(255) DEFAULT '|',
  `lang` varchar(5) DEFAULT NULL,
  `last_act` datetime DEFAULT NULL,
  `ip_address` varchar(50) DEFAULT NULL,
  PRIMARY KEY (`ID`),
  UNIQUE KEY `email` (`email`)
) ENGINE=MyISAM AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 ;

