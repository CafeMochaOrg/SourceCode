# Table: `wp_easymail_stats`
# Approximate rows expected in table: 9

# Delete any existing table `wp_easymail_stats`

DROP TABLE IF EXISTS `wp_easymail_stats`;

# Table structure of table `wp_easymail_stats`

CREATE TABLE `wp_easymail_stats` (
  `ID` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `recipient` int(11) unsigned NOT NULL,
  `newsletter` int(11) unsigned NOT NULL,
  `added_on` datetime NOT NULL,
  `request` text,
  PRIMARY KEY (`ID`),
  KEY `newsletter` (`newsletter`),
  KEY `recipient` (`recipient`)
) ENGINE=MyISAM AUTO_INCREMENT=10 DEFAULT CHARSET=utf8 ;

