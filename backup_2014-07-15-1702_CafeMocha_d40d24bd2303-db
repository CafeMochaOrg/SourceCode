# Table: `wp_redirection_modules`
# Approximate rows expected in table: 2

# Delete any existing table `wp_redirection_modules`

DROP TABLE IF EXISTS `wp_redirection_modules`;

# Table structure of table `wp_redirection_modules`

CREATE TABLE `wp_redirection_modules` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `type` varchar(20) NOT NULL DEFAULT '',
  `name` varchar(50) NOT NULL DEFAULT '',
  `options` mediumtext,
  PRIMARY KEY (`id`),
  KEY `name` (`name`),
  KEY `type` (`type`)
) ENGINE=MyISAM AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 ;

