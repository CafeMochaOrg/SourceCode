# Table: `wp_redirection_404`
# Approximate rows expected in table: 2570

# Delete any existing table `wp_redirection_404`

DROP TABLE IF EXISTS `wp_redirection_404`;

# Table structure of table `wp_redirection_404`

CREATE TABLE `wp_redirection_404` (
  `id` int(11) unsigned NOT NULL AUTO_INCREMENT,
  `created` datetime NOT NULL,
  `url` varchar(255) NOT NULL DEFAULT '',
  `agent` varchar(255) DEFAULT NULL,
  `referrer` varchar(255) DEFAULT NULL,
  `ip` int(10) unsigned NOT NULL,
  PRIMARY KEY (`id`),
  KEY `created` (`created`),
  KEY `url` (`url`),
  KEY `ip` (`ip`),
  KEY `referrer` (`referrer`)
) ENGINE=MyISAM AUTO_INCREMENT=2571 DEFAULT CHARSET=utf8 ;

