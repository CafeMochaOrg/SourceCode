# Table: `wp_ivrss_plugin`
# Approximate rows expected in table: 4

# Delete any existing table `wp_ivrss_plugin`

DROP TABLE IF EXISTS `wp_ivrss_plugin`;

# Table structure of table `wp_ivrss_plugin`

CREATE TABLE `wp_ivrss_plugin` (
  `ivrss_id` int(11) NOT NULL AUTO_INCREMENT,
  `ivrss_path` text CHARACTER SET utf8 COLLATE utf8_bin NOT NULL,
  `ivrss_link` text CHARACTER SET utf8 COLLATE utf8_bin NOT NULL,
  `ivrss_target` varchar(50) NOT NULL,
  `ivrss_title` varchar(500) NOT NULL,
  `ivrss_order` int(11) NOT NULL,
  `ivrss_status` varchar(10) NOT NULL,
  `ivrss_type` varchar(100) NOT NULL,
  `ivrss_extra1` varchar(100) NOT NULL,
  `ivrss_extra2` varchar(100) NOT NULL,
  `ivrss_date` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  PRIMARY KEY (`ivrss_id`)
) ENGINE=MyISAM AUTO_INCREMENT=5 DEFAULT CHARSET=latin1 ;

